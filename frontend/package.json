{
  "name": "my-first-serveless-frontend",
  "version": "1.0.0",
  "description": "A serverless url shortener to reduce links",
  "main": "src/server/serverless.js",
  "author": "Gregorio Alessio Costa",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.688.0",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "env-var": "^6.1.1",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serverless-http": "^2.5.0",
    "uuid": "^8.1.0"
