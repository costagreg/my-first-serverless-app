    "jest": "^26.0.1",
    "node-sass": "4.14.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "0.6.1",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "moduleNameMapper": {
